#include<iostream>
using namespace std;

int main()
 {   
    int age;
    cout<<"enter your age";
    cin>>age;

    if(age>=18){
        cout<<"you are eligible for voting";
    }

    else{
        cout<<"you are not eligible for voting";
    }

    int no;
    cout<<"enter the no";
    cin>>no;
    
    if(no%2==0){
        cout<<"no is even";
    }

    else{
        cout<<"no is odd";
    }

    int marks;
    cout<<"enter the marks: ";
    cin>>marks;
    if(marks>=90){
        cout<<"Grade A";
    }
    else if(marks>=80 && marks<90){
        cout<<"Grade B";
    }
    else{
        cout<<"Grade C";
    }
    char alph;
    cout<<"enter the alphabet: ";
    cin>>alph;
    if(alph>=65 && alph<=90){  // or if(alph>='A' && alph>='Z')
        cout<<"Upperrcase";
    }
    else{
        cout<<"Lowercase";
    }
    int n,sum=0;
    cout<<"enter the value of n: ";
    cin>>n;
     for(int i=1;i<=n;i++){
       if(i%2!=0)
       {
       sum=sum+i;
        }
      }
        cout<<"sum is : "<<sum;
    int n;
    bool isprime=true;
    cout<<"enter the no";
    cin>>n;
     for(int i=2;i<=n-1;i++){
      if(n%i==0){
        isprime=false;
        break;
      }
     }
      if(isprime=true)
        cout << "The no is prime";
    else
        cout << "The no is not prime";

    int n;
    int sum=0;
    cout<<"enter the no";
    cin>>n;
    for(int i=1;i<=n;i++){
      if(i%3==0){
        sum=sum+i;
      }
    } 
    cout<<"sum is: "<<sum;

    int n;
    int fact=1;
    cout<<"enter the value of n: ";
    cin>>n;
    for(int i=1;i<=n;i++){
      fact=fact*i;
    }
    cout<<"factorial is: "<<fact;
    return 0;
}
